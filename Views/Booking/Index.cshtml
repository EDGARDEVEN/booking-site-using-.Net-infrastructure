@model IEnumerable<Booking>

<h1>Existing Bookings</h1>
@foreach (var booking in Model)
 {
                <tr>
                    <td>@booking.Id</td>
                    <td>@booking.CustomerName</td>
                    <td>@booking.BookingDate.ToShortDateString()</td>
                </tr>
            }
        </tbody>
    </table>

else
{
    <p>No bookings found.</p>
}
